\r\n</template>\r\n\r\n<script>\r\nimport logo from \"../../assets/logo.svg\";\r\n\r\nexport default {\r\n  name: \"MobileHeader\",\r\n  data() {\r\n    return {\r\n      status: false,\r\n      fall: false,\r\n      logo,\r\n    };\r\n  },\r\n  methods: {\r\n    scrollToTop() {\r\n      window.scrollTo(0, 0);\r\n    },\r\n\r\n    handleSideMenu() {\r\n      this.status = !this.status;\r\n      const navbar = document.querySelector(\".mobile-header\");\r\n      if (navbar.classList.contains(\"show-navbar\")) {\r\n        navbar.classList.remove(\"show-navbar\");\r\n      } else {\r\n        navbar.classList.add(\"show-navbar\");\r\n      }\r\n    },\r\n\r\n    handleMenuFall() {\r\n      this.fall= !this.fall;\r\n\r\n      const sideMenu = document.querySelector(\".side-menu\");\r\n      if (sideMenu.classList.contains(\"tall\")) {\r\n        sideMenu.classList.remove(\"tall\");\r\n      } else {\r\n        sideMenu.classList.add(\"tall\");\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n@media screen and (max-width: 799px) {\r\n  .logo {\r\n    padding-top: 1vh;\r\n    height: 5vh;\r\n  }\r\n\r\n  .logo-row {\r\n    display: flex;\r\n    width: 90vw;\r\n    margin: 0vw 5vw;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n  }\r\n\r\n  .btns {\r\n    display: flex;\r\n    align-items: center;\r\n    gap: 5vw;\r\n  }\r\n\r\n  .menu {\r\n    display: block;\r\n\r\n    font-size: var(--link_size);\r\n    color: var(--link_color);\r\n    padding-top: 1vh;\r\n  }\r\n\r\n  .mobile-header {\r\n    position: fixed;\r\n    width: 0vw;\r\n    z-index: 10;\r\n    top: 0;\r\n    left: 0;\r\n    margin: 0vw 0vw;\r\n    transition: 1s all ease;\r\n  }\r\n\r\n  .navbar {\r\n    display: block;\r\n    margin-top: 10vw;\r\n    list-style: none;\r\n    overflow: hidden;\r\n    transition: 1s all ease;\r\n  }\r\n\r\n  @keyframes change {\r\n    0% {\r\n      width: 0vw;\r\n      opacity: 0;\r\n    }\r\n\r\n    100% {\r\n      width: 80vw;\r\n      opacity: 1;\r\n      background-color: var(--main_color);\r\n    }\r\n  }\r\n\r\n  ul {\r\n    margin: 0vw;\r\n    padding: 0vw;\r\n    padding-left: 5vw;\r\n  }\r\n\r\n  .show-navbar {\r\n    display: block;\r\n    width: 100vw;\r\n    height: 100vh;\r\n    background-color: rgba(250, 250, 250, 0.7);\r\n    list-style: none;\r\n    animation: change 5s linear infinite;\r\n  }\r\n\r\n  a {\r\n    text-decoration: none;\r\n    font-size: var(--link_size);\r\n  }\r\n\r\n  li {\r\n    margin: 5vw 2vw;\r\n    text-align: left;\r\n    color: var(--link_color);\r\n    font-size: var(--link_size);\r\n    font-weight: bold;\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}