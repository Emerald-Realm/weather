r\n    };\r\n  },\r\n  methods: {\r\n    prevIndex() {\r\n      if (this.index == 0) {\r\n        newIndex = length;\r\n      } else {\r\n        newIndex = this.index - 1;\r\n      }\r\n      this.index = newIndex;\r\n      // changing backgroud with respect to index value\r\n      this.styles = {\r\n        ...this.styles,\r\n        backgroundImage: `url(${productList[this.index].image})`,\r\n      };\r\n    },\r\n\r\n    nextIndex() {\r\n      if (this.index == length) {\r\n        newIndex = 0;\r\n      } else {\r\n        newIndex = this.index + 1;\r\n      }\r\n      setInterval(() => {\r\n        newIndex++\r\n      }, 2000)\r\n      this.index = newIndex;\r\n      // changing backgroud with respect to index value\r\n      this.styles = {\r\n        ...this.styles,\r\n        backgroundImage: `url(${productList[this.index].image})`,\r\n      };\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.carousel {\r\n  height: 50vh;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  /* gap: 86vw; */\r\n}\r\nbutton {\r\n  padding: 0.5vw;\r\n  background-color: white;\r\n  color: var(--main_color);\r\n  border: none;\r\n  border-radius: 10%;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: var(--main_color);\r\n  color: white;\r\n}\r\n\r\n</style>\r\n"]}]}